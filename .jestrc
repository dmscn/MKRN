{
	"testMatch": [
		"**/__tests__/**/*.test.j{"
	],
  "modulePathIgnorePatterns": [
    "/lib/",
    "/node_modules/",
		"/flow-typed/",
	"/dist"
  ],
  "persistModuleRegistryBetweenSpecs": true,
  "preprocessorIgnorePatterns": [
    "/node_modules/",
		"/flow-typed/",
	"/dist"
  ],
  "rootDir": "",
  "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
  "testPathDirs": [
    "<rootDir>/src/"
  ],
  "unmockedModulePathPatterns": 
    "<rootDir>/node_modules/",
    "<rootDir>/src/",
	"<rootDir>/dist"
  ],
	"collectCoverageFrom": [
		"/src/**/*.js"
	],
	"coveragePathIgnorePatterns": [
		"/node_modules/",
		"/flow-typed/"
		"/src/lib/env",
		"/src/bin",
		"**/__tests__/**",
		"/dist"
	]
}
