{
  "sourceMaps": "both",
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread",
    "@babel/plugin-proposal-decorators"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    ["flow"]
  ],
  "env": {
    "production": {
      "plugins": [
        "source-map-support"
      ],
      "ignore": [
        "**/__tests__/**"
      ]
    }
  }
}